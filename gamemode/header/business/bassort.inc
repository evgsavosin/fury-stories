enum E_BUSINESS_ASSORT_DATA
{
	ba_item_id,
	ba_item_param,
	ba_cost
}

new bassort_data				[	MAX_BUSINESSES		][	MAX_BUSINESS_ASSORT][	E_BUSINESS_ASSORT_DATA	];


Clear:all_bassort_data(index)
{
	for (new i; i < MAX_BUSINESS_ASSORT; i++)
	{
		Clear:bassort_data(index, i);
	}
}

Clear:bassort_data(bindex, index)
{
	bassort_data[bindex][index][ba_item_id] =
	bassort_data[bindex][index][ba_item_param] =
	bassort_data[bindex][index][ba_cost] = 0;
}

Create:bassort_data(bindex, asindex)
{
	new index = GetFreeIndexForBusinessAssort(bindex);
				
	if (index == INVALID_PARAM)
	{
		return 1;
	}
			
	format:g_big_string("INSERT INTO " #__BASSORT " \
		(ba_business_id, ba_item_id, ba_item_param, ba_cost, ba_created_time) \
			VALUES \
		(%i, %i, %i, %i, %i)",
		GetBusinessId(bindex),
		business_assort_list[asindex][bi_item_id],
		business_assort_list[asindex][bi_item_param],
		business_assort_list[asindex][bi_cost],
		gettime()
	);
	
	mysql_tquery(db, g_big_string, "OnBusinessAssortCreate", "iii", bindex, asindex, index);
	return 1;
}

function OnBusinessAssortCreate(bindex, asindex, index)
{
	bassort_data[bindex][index][ba_item_id] = business_assort_list[asindex][bi_item_id];
	bassort_data[bindex][index][ba_item_param] = business_assort_list[asindex][bi_item_param];
	bassort_data[bindex][index][ba_cost] = business_assort_list[asindex][bi_cost];
	
	printf("[OnBusinessAssortCreate]: Assortment item has been created for business. (ID:%i, ItemId:%i, Param:%i, Cost:%i)", 
		GetBusinessId(bindex),
		business_assort_list[asindex][bi_item_id],
		business_assort_list[asindex][bi_item_param],
		business_assort_list[asindex][bi_cost]
	);
	
	return 1;
}

Update:bassort_int(bindex, index, column[], _: value, bool: is_pquery = false)
{
	clean:<g_string>;
	
	format:g_string("UPDATE " #__BASSORT " SET %s = %d WHERE \
	ba_business_id = %i AND ba_item_id = %i AND ba_item_param = %i \
	LIMIT 1",
		column,
		value,
		GetBusinessId(bindex),
		bassort_data[bindex][index][ba_item_id],
		bassort_data[bindex][index][ba_item_param]
	);
	
	return !is_pquery ? mysql_tquery(db, g_string) : mysql_pquery(db, g_string);
}

Remove:bassort_data(bindex, index)
{	
	format:g_string("DELETE FROM " #__BASSORT " WHERE \
	ba_business_id = %d AND ba_item_id = %i AND ba_item_param = %i \
	LIMIT 1",
		GetBusinessId(bindex),
		bassort_data[bindex][index][ba_item_id],
		bassort_data[bindex][index][ba_item_param]
	);
	
	mysql_tquery(db, g_string);
	
	Clear:bassort_data(bindex, index);
}

Remove:all_bassort_data(index)
{
	format:g_string("DELETE FROM " #__BASSORT " WHERE ba_business_id = %i",
		GetBusinessId(index)
	);
	
	mysql_tquery(db, g_string);
	
	Clear:all_bassort_data(index);
}